{
  "extends": "airbnb",
  "env": {
    "browser":true,
    "jest": true
  },
  "rules": {
    "func-names": ["error", "never"],
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0,
    "class-methods-use-this": 0,
    "no-use-before-define": 0,
    "no-constant-condition": ["error", { "checkLoops": false }]
  }
}
